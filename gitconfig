[push]
	default = matching
[core]
	autocrlf = input
	excludesfile = ~/.dotfiles/gitignore_global
[color]
	diff = auto
	status = auto
	branch = auto
	ui = true
[alias]
	a = add
	aa = add --all
	br = branch
	ci = commit -a
	co = checkout
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	graph = log --graph --pretty=format:'%Cred%h%Creset %an: %s - %Creset %C(yellow)%d%Creset %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
[include]
  path = ~/.dotfiles/gitconfig.local
[apply]
   whitespace = nowarn
[mergetool]
  keepBackup = false
[help]
	autocorrect = 1